{
  "Lorentz Factor": "$\\gamma = \\frac{1}{\\sqrt{1 - \\frac{v^2}{c^2}}}$",
  "Relativistic Time Dilation": "$t = \\gamma t_0$",
  "Relativistic Length Contraction": "$L = \\frac{L_0}{\\gamma}$",
  "Relativistic Mass Dilation": "$m = \\gamma m_0$",
  "Relativistic Energy": "$E = \\sqrt{(pc)^2 + (mc^2)^2}$",
  "Relativistic Kinetic Energy": "$K = (\\gamma - 1)mc^2$",
  "Relativistic Velocity Addition": "$u = \\frac{u' + v}{1 + \\frac{u'v}{c^2}}$"
}





