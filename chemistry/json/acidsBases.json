{
  "Hydrochloric acid": "$HCl$ - Strong acid",
  "Hydrobromic acid": "$HBr$ - Strong acid",
  "Hydroiodic acid": "$HI$ - Strong acid",
  "Nitric acid": "$HNO_3$ - Strong acid",
  "Sulfuric acid": "$H_2SO_4$ - Strong acid (first proton)",
  "Perchloric acid": "$HClO_4$ - Strong acid",
  "Chloric acid": "$HClO_3$ - Strong acid",
  "Acetic acid": "$CH_3COOH$ or $HC_2H_3O_2$ - Weak acid",
  "Carbonic acid": "$H_2CO_3$ - Weak acid",
  "Phosphoric acid": "$H_3PO_4$ - Weak acid (all three protons)",
  "Hydrofluoric acid": "$HF$ - Weak acid",
  "Nitrous acid": "$HNO_2$ - Weak acid",
  "Sulfurous acid": "$H_2SO_3$ - Weak acid",
  "Hypochlorous acid": "$HClO$ - Weak acid",
  "Cyanic acid": "$HCN$ - Weak acid",
  "Formic acid": "$HCOOH$ - Weak acid",
  "Oxalic acid": "$H_2C_2O_4$ - Weak acid",
  "Sodium hydroxide": "$NaOH$ - Strong base",
  "Potassium hydroxide": "$KOH$ - Strong base",
  "Lithium hydroxide": "$LiOH$ - Strong base",
  "Rubidium hydroxide": "$RbOH$ - Strong base",
  "Cesium hydroxide": "$CsOH$ - Strong base",
  "Calcium hydroxide": "$Ca(OH)_2$ - Strong base",
  "Strontium hydroxide": "$Sr(OH)_2$ - Strong base",
  "Barium hydroxide": "$Ba(OH)_2$ - Strong base",
  "Ammonia": "$NH_3$ - Weak base",
  "Methylamine": "$CH_3NH_2$ - Weak base",
  "Ethylamine": "$C_2H_5NH_2$ - Weak base",
  "Aniline": "$C_6H_5NH_2$ - Weak base",
  "Pyridine": "$C_5H_5N$ - Weak base",
  "Trimethylamine": "$(CH_3)_3N$ - Weak base"
}
